{
   "name": "tiktok-clone-api",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "build": "tsc -p .",
      "migrate": "yarn build && yarn typeorm migration:generate ./src/migrations/structure -d ./src/utils/data-source.ts",
      "db:push": "yarn build && yarn typeorm migration:run -d src/utils/data-source.ts"
   },
   "devDependencies": {
      "@types/config": "^3.3.0",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/morgan": "^1.9.4",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.59.11",
      "@typescript-eslint/parser": "^5.60.0",
      "eslint": "^8.42.0",
      "morgan": "^1.10.0",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/jsonwebtoken": "^9.0.2",
      "argon2": "^0.30.3",
      "config": "^3.3.9",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.1.4",
      "envalid": "^7.3.1",
      "express": "^4.17.2",
      "jsonwebtoken": "^9.0.0",
      "multer": "^1.4.5-lts.1",
      "pg": "^8.4.0",
      "redis": "^4.6.7",
      "reflect-metadata": "^0.1.13",
      "sharp": "^0.32.1",
      "socket.io": "^4.7.2",
      "typeorm": "^0.3.16",
      "zod": "^3.21.4"
   }
}
